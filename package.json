{
  "name": "fbla-csa-manager",
  "productName": "CSA Manager",
  "version": "1.0.0",
  "description": "A cross-platform electron-based app to track hours for the FBLA community Service Awards program.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "Karthik Bandagonda",
  "license": "MIT",
  "devDependencies": {
    "electron": "^8.1.1",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "bootstrap": "^4.4.1",
    "datatables.net-bs4": "^1.10.20",
    "datatables.net-select-bs4": "^1.3.1",
    "electron-store": "^5.1.1",
    "electron-util": "^0.13.1",
    "jquery": "^3.4.1"
  },
  "build": {
    "appId": "com.csa.manager.app",
    "win": {
      "target": [
        "portable"
      ]
    },
    "portable": {
      "artifactName": "CSA Manager.exe"
    },
    "asar": false
  }
}
